function doSquareCalculation(number1, number2) {
    const number1Squared = number1 * number1;
    const number2Squared = number2 * number2;
    const sum = number1Squared + number2Squared;
    const sumSquared = sum * sum;
    console.log(sumSquared);
};

doSquareCalculation(4, 8);


const docalculation = function (number1, number2) {
    const number1Squared = number1 * number1;
    const number2Squared = number2 * number2;
    const sum = number1Squared + number2Squared;
    const sumSquared = sum * sum;
    const theresult = sumSquared + 1;
    console.log(theresult);
};

docalculation(4, 8);


const doSquareCalculation5 = (number1, number2) => {
    const number1Squared = number1 * number1;
    const number2Squared = number2 * number2;
    const sum = number1Squared + number2Squared;
    const sumSquared = sum * sum;
    const theresult = sumSquared + 1;
    console.log(theresult);
};

doSquareCalculation5(4, 8);
